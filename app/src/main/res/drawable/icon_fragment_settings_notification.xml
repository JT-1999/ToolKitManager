<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="100dp"
                android:height="100dp" />
        </shape>
    </item>
    <item
        android:bottom="32dp"
        android:left="18dp"
        android:right="48dp"
        android:top="32dp">
        <shape android:shape="rectangle">
            <size
                android:width="34dp"
                android:height="36dp" />
            <corners android:radius="10dp" />
            <solid android:color="@color/color_fragment_settings_notification" />
        </shape>
    </item>
    <item
        android:bottom="23dp"
        android:left="33dp"
        android:right="13dp"
        android:top="23dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <size
                    android:width="54dp"
                    android:height="54dp" />
                <corners android:radius="10dp" />
                <solid android:color="@color/color_fragment_settings_notification" />
            </shape>
        </rotate>
    </item>
    <item
        android:bottom="0dp"
        android:left="64dp"
        android:right="0dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <size
                android:width="36dp"
                android:height="100dp" />
            <solid android:color="@color/color_universe_foreground" />
        </shape>
    </item>
    <item android:left="28dp">
        <shape
            android:innerRadius="14dp"
            android:shape="ring"
            android:thickness="8dp"
            android:useLevel="false">
            <solid android:color="@color/color_fragment_settings_notification" />
        </shape>
    </item>
    <item
        android:bottom="0dp"
        android:left="64dp"
        android:right="32dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <size
                android:width="4dp"
                android:height="100dp" />
            <solid android:color="@color/color_universe_foreground" />
        </shape>
    </item>
</layer-list>