<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size android:width="100dp" android:height="100dp"/>
            <solid android:color="@color/color_universe_foreground"/>
        </shape>
    </item>
    <item
        android:top="16dp"
        android:bottom="26dp"
        android:left="14dp"
        android:right="14dp">
        <shape android:shape="rectangle">
            <size android:width="72dp" android:height="58dp"/>
            <solid android:color="@color/color_activity_main_message_icon"/>
            <corners
                android:bottomLeftRadius="0dp"
                android:radius="8dp"/>
        </shape>
    </item>
    <item
        android:top="45dp"
        android:bottom="25dp"
        android:left="10dp"
        android:right="60dp">
        <rotate
            android:fromDegrees="45"
            android:pivotX="0"
            android:pivotY="0">
            <shape android:shape="rectangle">
                <size android:width="30dp" android:height="30dp"/>
                <solid android:color="@color/color_activity_main_message_icon"/>
            </shape>
        </rotate>
    </item>
    <item
        android:top="0dp"
        android:bottom="0dp"
        android:left="0dp"
        android:right="90dp">
        <shape android:shape="rectangle">
            <size android:width="14dp" android:height="100dp"/>
            <solid android:color="@color/color_universe_foreground"/>
        </shape>
    </item>
    <item
        android:top="28dp"
        android:bottom="64dp"
        android:left="30dp"
        android:right="30dp">
        <shape android:shape="rectangle">
            <size android:width="40dp" android:height="8dp"/>
            <solid android:color="@color/color_universe_foreground"/>
        </shape>
    </item>
    <item
        android:top="40dp"
        android:bottom="52dp"
        android:left="30dp"
        android:right="30dp">
        <shape android:shape="rectangle">
            <size android:width="40dp" android:height="8dp"/>
            <solid android:color="@color/color_universe_foreground"/>
        </shape>
    </item>
    <item
        android:top="52dp"
        android:bottom="40dp"
        android:left="30dp"
        android:right="30dp">
        <shape android:shape="rectangle">
            <size android:width="40dp" android:height="8dp"/>
            <solid android:color="@color/color_universe_foreground"/>
        </shape>
    </item>
</layer-list>