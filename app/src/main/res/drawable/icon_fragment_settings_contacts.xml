<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size android:width="100dp" android:height="100dp"/>
        </shape>
    </item>
    <item
        android:left="60dp"
        android:bottom="60dp">
        <shape
            android:shape="ring"
            android:innerRadius="58dp"
            android:thickness="10dp"
            android:useLevel="false">
            <solid android:color="@color/color_fragment_settings_contacts"/>
        </shape>
    </item>
    <item
        android:top="0dp"
        android:bottom="70dp"
        android:left="0dp"
        android:right="70dp">
        <shape android:shape="rectangle">
            <size android:width="30dp" android:height="30dp"/>
            <solid android:color="@color/color_universe_foreground"/>
        </shape>
    </item>
    <item
        android:top="70dp"
        android:bottom="0dp"
        android:left="70dp"
        android:right="0dp">
        <shape android:shape="rectangle">
            <size android:width="30dp" android:height="30dp"/>
            <solid android:color="@color/color_universe_foreground"/>
        </shape>
    </item>
    <item
        android:top="20dp"
        android:bottom="55dp"
        android:left="15dp"
        android:right="60dp">
        <rotate android:fromDegrees="-20">
            <shape android:shape="rectangle">
                <size android:width="25dp" android:height="25dp"/>
                <corners android:radius="8dp"/>
                <solid android:color="@color/color_fragment_settings_contacts"/>
            </shape>
        </rotate>
    </item>
    <item
        android:top="60dp"
        android:bottom="15dp"
        android:left="55dp"
        android:right="20dp">
        <rotate android:fromDegrees="20">
            <shape android:shape="rectangle">
                <size android:width="25dp" android:height="25dp"/>
                <corners android:radius="8dp"/>
                <solid android:color="@color/color_fragment_settings_contacts"/>
            </shape>
        </rotate>
    </item>
</layer-list>