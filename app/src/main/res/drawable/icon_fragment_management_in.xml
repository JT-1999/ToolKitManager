<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size android:width="100dp" android:height="100dp"/>
            <solid android:color="@color/color_fragment_management_in"/>
        </shape>
    </item>
    <item
        android:top="30dp"
        android:bottom="14dp"
        android:left="20dp"
        android:right="20dp">
        <shape android:shape="rectangle">
            <size android:height="56dp" android:width="60dp"/>
            <corners android:radius="8dp"/>
            <stroke android:width="4dp" android:color="@color/color_universe_foreground"/>
        </shape>
    </item>
    <item
        android:top="29dp"
        android:bottom="65dp"
        android:left="40dp"
        android:right="40dp">
        <shape android:shape="rectangle">
            <size android:height="6dp" android:width="20dp"/>
            <solid android:color="@color/color_fragment_management_in"/>
        </shape>
    </item>
    <item
        android:top="46dp"
        android:bottom="34dp"
        android:left="40dp"
        android:right="40dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <size android:height="20dp" android:width="20dp"/>
                <solid android:color="@color/color_universe_foreground"/>
            </shape>
        </rotate>
    </item>
    <item
        android:top="36dp"
        android:bottom="44dp"
        android:left="30dp"
        android:right="30dp">
        <shape android:shape="rectangle">
            <size android:height="20dp" android:width="40dp"/>
            <solid android:color="@color/color_fragment_management_in"/>
        </shape>
    </item>
    <item
        android:top="16dp"
        android:bottom="44dp"
        android:left="48dp"
        android:right="48dp">
        <shape android:shape="rectangle">
            <size android:height="40dp" android:width="4dp"/>
            <corners android:topLeftRadius="2dp"
                android:topRightRadius="2dp"/>
            <solid android:color="@color/color_universe_foreground"/>
        </shape>
    </item>
</layer-list>