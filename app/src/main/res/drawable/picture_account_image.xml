            <size android:height="40dp" android:width="40dp"/>
            <solid android:color="@color/color_universe_background"/>
        </shape>
    </item>
    <item
        android:top="12dp"
        android:bottom="52dp"
        android:left="32dp"
        android:right="32dp">
        <shape android:shape="oval">
            <size android:height="36dp" android:width="36dp"/>
            <solid android:color="@color/color_fragment_management_in"/>
        </shape>
    </item>
</layer-list>