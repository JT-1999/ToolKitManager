<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="100dp"
                android:height="100dp" />
            <solid android:color="@color/color_fragment_management_purchase" />
        </shape>
    </item>
    <item
        android:bottom="50dp"
        android:left="54dp"
        android:right="42dp"
